<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <function model="res.partner" name="update_display_partner_accounts"/>

        <record id="seq_spec_payable_partner_account" model="ir.sequence">
            <field name="name">Account Payable</field>
            <field name="code">spec.account.payable</field>
            <field name="prefix">7</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>


        <record id="seq_spec_receivable_partner_account" model="ir.sequence">
            <field name="name">Account Receivable</field>
            <field name="code">spec.account.receivable</field>
            <field name="prefix">1</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>